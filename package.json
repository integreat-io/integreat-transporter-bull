  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@types/bull": "^4.10.0",
    "@types/debug": "^4.1.7",
    "bull": "^4.10.4",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@integreat/ts-dev-setup": "^4.0.1",
    "@types/sinon": "^10.0.14",
    "integreat": "^0.8.0-rc.4",
    "sinon": "^15.0.4"
  }
