  "homepage": "https://github.com/integreat-io/integreat-transporter-bull#readme",
  "engines": {
    "node": ">= 14"
  },
  "dependencies": {
    "@types/bull": "^3.15.4",
    "bull": "^3.29.0"
  },
  "devDependencies": {
    "@integreat/ts-dev-setup": "^2.1.0",
    "@types/sinon": "^10.0.2",
    "sinon": "^11.1.2"
  }
}
